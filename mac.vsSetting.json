{
    "vscode_custom_css.imports": [
        "file:///Users/Ruie/.vscodestyles.css"
    ],
    "vscode_custom_css.policy": true,
    "editor.fontSize": 12,
    "keyboard.dispatch": "keyCode",
    "window.zoomLevel": 0,
    "explorer.openEditors.visible": 1,
    "editor.tabSize": 3,
    "editor.fontFamily": "Roboto Mono Nerd Font Complete,Menlo, Monaco, 'Courier New', monospace",
    "terminal.integrated.fontFamily": "Roboto Mono for Powerline",
    "vim.leader": ",",
    "scm.diffDecorations": ["all", "gutter", "overview", "none"],
    "vim.insertModeKeyBindings": [
        {
            "before": ["k","j"],
            "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                {
                    "command": "workbench.action.files.save",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "x"
            ],
            "commands": [
                {
                    "command": "workbench.action.closeActiveEditor",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                {
                    "command": "actions.find",
                    "args": []
                }
            ]
        },
        {
            "before": ["K"],
            "after": ["5","k"]
        },
        {
            "before": ["J"],
            "after": ["5","j"]
        },
        {
            "before": ["H"],
            "after": ["^"]
        },
        {
            "before": ["L"],
            "after": ["g","_"]
        },
        {
            "before": ["<leader>",","],
            "after": ["<Esc>"]
        },
        {
            "before": [";"],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.showCommands",
                    "args": []
                }
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["K"],
            "after": ["5","k"]
        },
        {
            "before": ["J"],
            "after": ["5","j"]
        },
        {
            "before": ["H"],
            "after": ["^"]
        },
        {
            "before": ["L"],
            "after": ["g","_"]
        },
        {
            "before": ["<leader>",","],
            "after": ["<Esc>"]
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                {
                    "args": [],
                    "command": "editor.action.indentLines"
                }
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                {
                    "args": [],
                    "command": "editor.action.outdentLines"
                }
            ]
        },
    ],
    "scrolloff.scrolloff" : 15,
    "scrolloff.alwaysCenter": false,
    "editor.snippetSuggestions": "top",
    "editor.renderLineHighlight": "all",
    "workbench.activityBar.visible": false,
    "workbench.sideBar.location": "left",
    "workbench.statusBar.visible": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": false,
    "workbench.colorCustomizations": {
        "scrollbarSlider.background": "#212121",
        "scrollbarSlider.activeBackground": "#212121",
        "scrollbarSlider.hoverBackground": "#212121",
        "statusBar.background": "#B48EAD",
        "statusBar.noFolderBackground": "#B48EAD",
        "statusBar.debuggingBackground": "#B48EAD"
    },
    "editor.selectionHighlight": true,
    "git.confirmSync": false,
    "editor.renderControlCharacters": true,
    "workbench.startupEditor": "newUntitledFile",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "bracketPairColorizer.scopeLineRelativePosition": true,
    "files.exclude": {
        "/.git": true,
        "/.svn": true,
        "/.hg": true,
        "/CVS": true,
        "/.DS_Store": true,
        "/.history": true,
        "/.github": true,
        "**/.vscode": true,
        "**/.expo": true,
        "**/node_modules": true
    },
    "materialTheme.fixIconsRunning": false,
    // Remove Scrollborder
    "editor.overviewRulerBorder": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.fontAliasing": "default",
    "window.closeWhenEmpty": true,
    "terminal.integrated.cursorBlinking": true,
    "bracketPairColorizer.highlightActiveScope": false,
    "bracketPairColorizer.showBracketsInGutter": true,
    "bracketPairColorizer.showBracketsInRuler": false,
    "material-icon-theme.hidesExplorerArrows": true,
    "bracketPairColorizer.showHorizontalScopeLine": false,
    "vim.enableNeovim": true,
    "workbench.colorTheme": "Ruie Dark",
    "git.enableSmartCommit": true,
    "search.location": "panel",
    "workbench.iconTheme": "Monokai Pro Icons",
    "javascript.updateImportsOnFileMove.enabled": "always"
}
